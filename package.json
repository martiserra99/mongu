{
  "version": "2.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "mongu",
  "description": "Use use mongo-like expressions in JavaScript",
  "author": "Marti Serra Molina",
  "module": "dist/mongu.esm.js",
  "size-limit": [
    {
      "path": "dist/mongu.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mongu.esm.js",
      "limit": "10 KB"
    }
  ],
  "keywords": [
    "javascript",
    "expression",
    "transform",
    "project",
    "modify",
    "update",
    "mongo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/martiserra99/mongu"
  },
  "homepage": "https://github.com/martiserra99/mongu",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^10.0.2",
    "husky": "^8.0.3",
    "size-limit": "^10.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
